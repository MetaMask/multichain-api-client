{
  "name": "@metamask/multichain-api-client",
  "version": "0.0.0",
  "description": "The MetaMask Node module template",
  "homepage": "https://github.com/MetaMask/multichain-api-client#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/multichain-api-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/multichain-api-client.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/helpers/metamaskExtensionId": {
      "types": "./dist/helpers/metamaskExtensionId.d.mts",
      "import": "./dist/helpers/metamaskExtensionId.mjs",
      "require": "./dist/helpers/metamaskExtensionId.cjs"
    },
    "./dist/helpers/providerDetection": {
      "types": "./dist/helpers/providerDetection.d.mts",
      "import": "./dist/helpers/providerDetection.mjs",
      "require": "./dist/helpers/providerDetection.cjs"
    },
    "./dist/helpers/utils": {
      "types": "./dist/helpers/utils.d.mts",
      "import": "./dist/helpers/utils.mjs",
      "require": "./dist/helpers/utils.cjs"
    },
    "./dist/transports/externallyConnectableTransport": {
      "types": "./dist/transports/externallyConnectableTransport.d.mts",
      "import": "./dist/transports/externallyConnectableTransport.mjs",
      "require": "./dist/transports/externallyConnectableTransport.cjs"
    },
    "./dist/transports/streamTransport": {
      "types": "./dist/transports/streamTransport.d.mts",
      "import": "./dist/transports/streamTransport.mjs",
      "require": "./dist/transports/streamTransport.cjs"
    },
    "./dist/multichainClient": {
      "types": "./dist/multichainClient.d.mts",
      "import": "./dist/multichainClient.mjs",
      "require": "./dist/multichainClient.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ts-bridge --project tsconfig.build.json --clean",
    "build:docs": "typedoc",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "yarn lint:biome && yarn lint:constraints && yarn lint:dependencies --check && yarn lint:changelog",
    "lint:changelog": "auto-changelog validate --prettier",
    "lint:constraints": "yarn constraints",
    "lint:dependencies": "depcheck && yarn dedupe",
    "lint:biome": "biome check",
    "lint:fix": "yarn lint:biome --fix && yarn format:fix && yarn lint:constraints --fix && yarn lint:dependencies && yarn lint:changelog",
    "prepack": "./scripts/prepack.sh",
    "test": "vitest && attw --pack --profile node16 && yarn test:types",
    "test:types": "tsd  --files ./src/tests/index.test-d.ts",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.4",
    "@biomejs/biome": "1.9.4",
    "@lavamoat/allow-scripts": "^3.0.4",
    "@lavamoat/preinstall-always-fail": "^2.0.0",
    "@metamask/auto-changelog": "^4.0.0",
    "@metamask/providers": "^20.0.0",
    "@ts-bridge/cli": "^0.6.3",
    "@types/chrome": "^0.0.307",
    "@vitest/coverage-istanbul": "^3.0.7",
    "@vitest/eslint-plugin": "^1.1.4",
    "@yarnpkg/types": "^4.0.0-rc.52",
    "depcheck": "^1.4.3",
    "prettier": "^3.5.3",
    "ts-node": "^10.7.0",
    "tsd": "^0.31.2",
    "typedoc": "^0.26.11",
    "typescript": "~5.7.3",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  },
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": "^18.20 || ^20.17 || >=22"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "vite>esbuild": false,
      "@biomejs/biome": false
    }
  }
}
